<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>PROBLEM_DESCRIPTION</key>
	<string>{@name} has {$start} {@items} and {@friend} gives them {$add} more. How many {@items} does {@name} have? Starting at {$start}, count on {$add}.</string>
	<key>INTERNAL_DESCRIPTION</key>
	<string>Count up from a certain number</string>
	<key>TOOL_KEY</key>
	<string>NLine</string>
	<key>START_VALUE</key>
	<string>{$start}</string>
	<key>MIN_VALUE</key>
	<string>0</string>
	<key>MAX_VALUE</key>
	<string>{$start+$add}</string>
	<key>EVAL_MODE</key>
	<integer>1</integer>
	<key>REJECT_MODE</key>
	<integer>0</integer>
	<key>EVAL_TARGET</key>
	<string>{$start+$add}</string>
	<key>HIDE_ALL_NUMBERS</key>
	<false/>
	<key>SHOW_NUMBERS_AT_INTERVALS</key>
	<array/>
	<key>HIDE_ALL_NOTCHES</key>
	<false/>
	<key>SHOW_NOTCHES_AT_INTERVALS</key>
	<array/>
	<key>HIDE_START_NUMBER</key>
	<false/>
	<key>HIDE_END_NUMBER</key>
	<false/>
	<key>HIDE_START_NOTCH</key>
	<false/>
	<key>HIDE_END_NOTCH</key>
	<false/>
	<key>DISABLE_SNAPPING_TO_NOTCH</key>
	<false/>
	<key>EVAL_PLUS_MINUS_THRESHOLD</key>
	<integer>0</integer>
	<key>COUNT_OUT_LOUD_AT_INTERVALS</key>
	<array>
		<integer>1</integer>
	</array>
	<key>SHOW_COUNT_HISTORY_AT_INTERVALS</key>
	<array/>
	<key>USER_POPULATED_NUMBERS</key>
	<array>
		<string>2</string>
		<string>4</string>
		<string>6</string>
		<string>8</string>
	</array>
	<key>RENDER_STITCHES_AT_INTERVAL</key>
	<integer>0</integer>
	<key>DVARS</key>
	<dict>
		<key>$start</key>
		<dict>
			<key>MAX</key>
			<string>7</string>
			<key>MIN</key>
			<string>1</string>
		</dict>
		<key>$add</key>
		<dict>
			<key>MAX</key>
			<string>10</string>
			<key>MIN</key>
			<string>1</string>
		</dict>
	</dict>
	<key>DSTRINGS</key>
	<dict>
		<key>@items</key>
		<dict>
			<key>MODE</key>
			<string>RANDOM</string>
			<key>DATA</key>
			<array>
				<string>sweets</string>
				<string>cookies</string>
				<string>crisps</string>
				<string>haribos</string>
			</array>
		</dict>
		<key>@friend</key>
		<dict>
			<key>MODE</key>
			<string>RANDOM</string>
			<key>DATA</key>
			<array>
				<string>Elizabeth</string>
				<string>Katie</string>
				<string>Carl</string>
			</array>
		</dict>
		<key>@name</key>
		<dict>
			<key>MODE</key>
			<string>RANDOM</string>
			<key>DATA</key>
			<array>
				<string>Sarah</string>
				<string>Claire</string>
				<string>Scott</string>
				<string>Gareth</string>
				<string>Dave</string>
				<string>Nick</string>
			</array>
		</dict>
	</dict>
</dict>
</plist>
