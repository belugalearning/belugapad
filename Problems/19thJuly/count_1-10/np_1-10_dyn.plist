<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>NUMBER_PICKER</key>
	<dict>
		<key>NUMBER_PICKER_DESCRIPTION</key>
		<string>{@name} has {$start} {@items} and {@friend} gives them {$add} more. How many {@items} does {@name} have? Starting at {$start}, count on {$add}.</string>
		<key>PICKER_LAYOUT</key>
		<integer>1</integer>
		<key>PICKER_ORIGIN_X</key>
		<integer>0</integer>
		<key>PICKER_ORIGIN_Y</key>
		<integer>0</integer>
		<key>SHOW_DROPBOX</key>
		<true/>
		<key>PICKER_EVAL_MODE</key>
		<integer>1</integer>
		<key>EVAL_VALUE</key>
		<string>{$start+$add}</string>
		<key>DSTRINGS</key>
		<dict>
			<key>@items</key>
			<dict>
				<key>MODE</key>
				<string>RANDOM</string>
				<key>DATA</key>
				<array>
					<string>sweets</string>
					<string>cookies</string>
					<string>crisps</string>
					<string>haribos</string>
				</array>
			</dict>
			<key>@friend</key>
			<dict>
				<key>MODE</key>
				<string>RANDOM</string>
				<key>DATA</key>
				<array>
					<string>Elizabeth</string>
					<string>Katie</string>
					<string>Carl</string>
				</array>
			</dict>
			<key>@name</key>
			<dict>
				<key>MODE</key>
				<string>RANDOM</string>
				<key>DATA</key>
				<array>
					<string>Sarah</string>
					<string>Claire</string>
					<string>Scott</string>
					<string>Gareth</string>
					<string>Dave</string>
					<string>Nick</string>
				</array>
			</dict>
		</dict>
		<key>DVARS</key>
		<dict>
			<key>$start</key>
			<dict>
				<key>MAX</key>
				<string>7</string>
				<key>MIN</key>
				<string>1</string>
			</dict>
			<key>$add</key>
			<dict>
				<key>MAX</key>
				<string>10</string>
				<key>MIN</key>
				<string>1</string>
			</dict>
		</dict>
	</dict>
</dict>
</plist>
