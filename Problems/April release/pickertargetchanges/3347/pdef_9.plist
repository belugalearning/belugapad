<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>TOOL_KEY</key>
	<string>ExprBuilder</string>
	<key>INTERNAL_DESCRIPTION</key>
	<string>Create a calculation which is the same as {$x} + {$y}. Make the numbers in the bracket as small as possible.</string>
	<key>EVAL_MODE</key>
	<string>1</string>
	<key>EVAL_TYPE</key>
	<string>ALL_ENABLED</string>
	<key>EXPR_STAGES</key>
	<array>
		<string>Create a calculation which is the same as {$x} + {$y}. Make the numbers in the bracket as small as possible.</string>
		<string>&lt;b:on number=&quot;1&quot;/&gt;
&lt;b:on number=&quot;2&quot; /&gt;
&lt;b:on number=&quot;3&quot; /&gt;
&lt;b:on number=&quot;4&quot; /&gt;
&lt;b:on number=&quot;5&quot; /&gt;
&lt;b:on number=&quot;6&quot; /&gt;
&lt;b:on number=&quot;7&quot; /&gt;
&lt;b:on number=&quot;8&quot; /&gt;
&lt;b:on number=&quot;9&quot; /&gt;
&lt;b:on number=&quot;10&quot; /&gt;</string>
		<string>&lt;b:on number=&quot;5&quot; enabled=&quot;no&quot;/&gt;
&lt;b:t&gt;(&lt;/b:t&gt;
&lt;b:on number=&quot;{$x/5}&quot; enabled=&quot;no&quot;/&gt;
&lt;b:op op=&quot;+&quot; /&gt;
&lt;b:on number=&quot;{$y/5}&quot; enabled=&quot;no&quot;/&gt;
&lt;b:t&gt;)&lt;/b:t&gt;
&lt;b:op op=&quot;=&quot; /&gt;
&lt;b:t&gt;{$x} + {$y}&lt;/b:t&gt;</string>
	</array>
	<key>DVARS</key>
	<array>
		<dict>
			<key>VAR_NAME</key>
			<string>$x</string>
			<key>SELECT_FROM</key>
			<array>
				<string>15</string>
				<string>20</string>
				<string>25</string>
			</array>
		</dict>
		<dict>
			<key>VAR_NAME</key>
			<string>$y</string>
			<key>SELECT_FROM</key>
			<array>
				<string>15</string>
				<string>30</string>
				<string>35</string>
				<string>40</string>
			</array>
		</dict>
	</array>
	<key>DSTRINGS</key>
	<dict>
		<key>@object</key>
		<dict>
			<key>MODE</key>
			<string>RANDOM</string>
			<key>DATA</key>
			<array>
				<string>apples</string>
				<string>sweets</string>
				<string>pens</string>
			</array>
		</dict>
	</dict>
	<key>REPEAT_AS_IS_MIN</key>
	<integer>2</integer>
	<key>ENABLE_CALCULATOR</key>
	<true/>
</dict>
</plist>
