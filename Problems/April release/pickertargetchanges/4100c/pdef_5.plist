<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>TOOL_KEY</key>
	<string>ExprBuilder</string>
	<key>INTERNAL_DESCRIPTION</key>
	<string>Create a calculation which is the same as {$x} + {$y}. Make the numbers in the bracket as small as possible.</string>
	<key>EVAL_MODE</key>
	<string>1</string>
	<key>EVAL_TYPE</key>
	<string>ALL_ENABLED</string>
	<key>EXPR_STAGES</key>
	<array>
		<string>&lt;b:t&gt;Write&lt;/b:t&gt;&lt;b:on numerator=&quot;{$x}&quot; denominator=&quot;{$y}&quot;/&gt;&lt;b:t&gt; as a division.&lt;/b:t&gt;</string>
		<string>&lt;b:on numerator=&quot;{$x}&quot; denominator=&quot;{$y}&quot;/&gt;&lt;b:oo value=&quot;=&quot;/&gt; &lt;b:on usePicker=&quot;yes&quot; pickerTarget=&quot;{$x}&quot;/&gt;&lt;b:oo value=&quot;/&quot;/&gt; &lt;b:on usePicker=&quot;yes&quot; pickerTarget=&quot;{$y}&quot;/&gt;</string>
	</array>
	<key>DVARS</key>
	<array>
		<dict>
			<key>VAR_NAME</key>
			<string>$x</string>
			<key>RECALL</key>
			<string>$x</string>
		</dict>
		<dict>
			<key>VAR_NAME</key>
			<string>$y</string>
			<key>RECALL</key>
			<string>$y</string>
		</dict>
	</array>
	<key>DSTRINGS</key>
	<dict>
		<key>@object</key>
		<dict>
			<key>MODE</key>
			<string>RANDOM</string>
			<key>DATA</key>
			<array>
				<string>apples</string>
				<string>sweets</string>
				<string>pens</string>
			</array>
		</dict>
	</dict>
</dict>
</plist>
