				<false/>
			</dict>
			<dict>
				<key>META_ANSWER_TEXT</key>
				<string>( {5*$a} รท {3*$a} ) x {3*$b}</string>
				<key>META_ANSWER_VALUE</key>
				<false/>
			</dict>
		</array>
