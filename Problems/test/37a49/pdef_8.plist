<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>TOOL_KEY</key>
	<string>ExprBuilder</string>
	<key>EVAL_TYPE</key>
	<string>ALL_PICKERS_CORRECT</string>
	<key>EXPR_STAGES</key>
	<array>
		<string>&lt;b:t&gt;{$x} {@obj} cost {$z} goobles. Fill in the gaps.&lt;/b:t&gt;</string>
		<string>&lt;b:on number=&quot;{$x}&quot; suffix=&quot; {@obj}&quot;/&gt;&lt;b:oo value=&quot;=&quot; /&gt;&lt;b:on number=&quot;{$z}&quot; suffix=&quot; goobles&quot;/&gt;</string>
		<string>&lt;b:on number=&quot;{$x+3}&quot; suffix=&quot; {@obj}&quot;/&gt;&lt;b:oo value=&quot;=&quot; /&gt;&lt;b:on usePicker=&quot;yes&quot; pickerTarget=&quot;{$x3*$y}&quot; suffix=&quot; goobles&quot;/&gt;</string>
		<string>&lt;b:on number=&quot;{$x+2}&quot; suffix=&quot; {@obj}&quot;/&gt;&lt;/b:t&gt;&lt;b:oo value=&quot;=&quot; /&gt;&lt;b:on usePicker=&quot;yes&quot; pickerTarget=&quot;{$x2*$y}&quot; suffix=&quot; goobles&quot;/&gt;</string>
	</array>
	<key>DVARS</key>
	<array>
		<dict>
			<key>VAR_NAME</key>
			<string>$x</string>
			<key>MIN</key>
			<string>3</string>
			<key>MAX</key>
			<string>6</string>
		</dict>
		<dict>
			<key>VAR_NAME</key>
			<string>$y</string>
			<key>MIN</key>
			<string>2</string>
			<key>MAX</key>
			<string>6</string>
		</dict>
		<dict>
			<key>VAR_NAME</key>
			<string>$x3</string>
			<key>VALUE</key>
			<string>{$x+3}</string>
		</dict>
		<dict>
			<key>VAR_NAME</key>
			<string>$x2</string>
			<key>VALUE</key>
			<string>{$x+2}</string>
		</dict>
		<dict>
			<key>VAR_NAME</key>
			<string>$z</string>
			<key>VALUE</key>
			<string>{$y*$x}</string>
		</dict>
	</array>
	<key>DSTRINGS</key>
	<dict>
		<key>@obj</key>
		<dict>
			<key>MODE</key>
			<string>RANDOM</string>
			<key>DATA</key>
			<array>
				<string>pens</string>
				<string>cakes</string>
				<string>sweets</string>
				<string>crayons</string>
			</array>
		</dict>
	</dict>
	<key>REPEAT_AS_IS_MIN</key>
	<string>3</string>
	<key>EVAL_MODE</key>
	<string>1</string>
</dict>
</plist>
