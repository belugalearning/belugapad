  <dict>
    <key>TOOL_KEY</key>
    <string>ExprBuilder</string>
    <key>INTERNAL_DESCRIPTION</key>
    <string>Express 4 x 8 as an addition.</string>
    <key>EXPR_STAGES</key>
    <array>
      <string>&lt;b:t&gt;Express &lt;/b:t&gt;
&lt;b:ot tag=&quot;a&quot;&gt;4&lt;/b:ot&gt;
&lt;b:t&gt;x&lt;/b:t&gt;
&lt;b:ot tag=&quot;b&quot;&gt;8&lt;/b:ot&gt;
&lt;b:t&gt;as an addition equation.&lt;/b:t&gt;</string>
      <string>&lt;b:commlkp lkpid=&quot;cl1&quot;&gt;
 &lt;b:lkpitem tag=&quot;a&quot;&gt;4&lt;/b:lkpitem&gt;
 &lt;b:lkpitem tag=&quot;b&quot;&gt;8&lt;/b:lkpitem&gt;
&lt;/b:commlkp&gt;

&lt;b:commot lkpid=&quot;cl1&quot; sample=&quot;8&quot; preftag=&quot;b&quot; /&gt;
&lt;b:oo value=&quot;+&quot; picker=&quot;yes&quot; /&gt;
&lt;b:commot lkpid=&quot;cl1&quot; sample=&quot;8&quot; preftag=&quot;b&quot; /&gt;
&lt;b:oo value=&quot;+&quot; picker=&quot;yes&quot; /&gt;
&lt;b:commot lkpid=&quot;cl1&quot; sample=&quot;8&quot; preftag=&quot;b&quot; /&gt;
&lt;b:oo value=&quot;+&quot; picker=&quot;yes&quot; /&gt;
&lt;b:commot lkpid=&quot;cl1&quot; sample=&quot;8&quot; preftag=&quot;b&quot; /&gt;</string>
    </array>
    <key>DVARS</key>
    <dict>
      <key>$object</key>
      <dict>
        <key>MIN</key>
        <string>4</string>
        <key>MAX</key>
        <string>9</string>
      </dict>
      <key>$decrementor</key>
      <dict>
        <key>MIN</key>
        <string>1</string>
        <key>MAX</key>
        <string>3</string>
      </dict>
      <key>$number</key>
      <dict>
        <key>VALUE</key>
        <string>{$object-$decrementor}</string>
      </dict>
    </dict>
