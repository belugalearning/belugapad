<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>FOUNDATION</key>
    <true/>
    <key>PROBLEM_DESCRIPTION</key>
    <string>Make {$x+$y}.</string>
    <key>INTERNAL_DESCRIPTION</key>
    <string>x+y</string>
    <key>TOOL_KEY</key>
    <string>FloatingBlock</string>
    <key>EVAL_MODE</key>
    <integer>0</integer>
    <key>SOLUTION</key>
    <string>{$x+$y}</string>
    <key>INIT_BUBBLES</key>
    <integer>2</integer>
    <key>INIT_OBJECTS</key>
    <array>
      <dict>
        <key>NUMBER</key>
        <string>{$x}</string>
      </dict>
      <dict>
        <key>NUMBER</key>
        <string>{$y}</string>
      </dict>
    </array>
    <key>BUBBLE_AUTO_OPERATE</key>
    <false/>
    <key>SHOW_MULTIPLE_CONTROLS</key>
    <true/>
    <key>SUPPORTED_OPERATORS</key>
    <array>
      <string>+</string>
    </array>
    <key>SHOW_INPUT_PIPE</key>
    <false/>
    <key>SHOW_SOLUTION_ON_PIPE</key>
    <true/>
    <key>DVARS</key>
    <array>
      <dict>
        <key>RETAIN</key>
        <true/>
        <key>VAR_NAME</key>
        <string>$x</string>
        <key>MIN</key>
        <string>1</string>
        <key>MAX</key>
        <string>3</string>
      </dict>
      <dict>
        <key>VAR_NAME</key>
        <string>$y</string>
        <key>RETAIN</key>
        <true/>
        <key>MIN</key>
        <string>1</string>
        <key>MAX</key>
        <string>3</string>
      </dict>
    </array>
  </dict>
</plist>
