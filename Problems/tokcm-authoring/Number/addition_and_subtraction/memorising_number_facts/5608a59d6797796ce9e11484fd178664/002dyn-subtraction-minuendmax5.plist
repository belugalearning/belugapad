<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>NUMBER_PICKER</key>
	<dict>
		<key>NUMBER_PICKER_DESCRIPTION</key>
		<string>What is {$minuend} {@operator} {$subtrahend}?</string>
		<key>INTERNAL_DESCRIPTION</key>
		<string>memorise subtraction facts with 5 &lt; minuend &lt; 10</string>
		<key>PICKER_LAYOUT</key>
		<integer>1</integer>
		<key>ANIMATE_FROM_PICKER</key>
		<true/>
		<key>SHOW_DROPBOX</key>
		<true/>
		<key>PICKER_EVAL_MODE</key>
		<integer>1</integer>
		<key>EVAL_VALUE</key>
		<string>{$minuend-$subtrahend}</string>
	</dict>
	<key>DSTRINGS</key>
	<dict>
		<key>@operator</key>
		<dict>
			<key>MODE</key>
			<string>RANDOM</string>
			<key>DATA</key>
			<array>
				<string>subtract</string>
				<string>take away</string>
				<string>-</string>
				<string>minus</string>
				<string>decreased by</string>
			</array>
		</dict>
	</dict>
	<key>DVARS</key>
	<array>
		<dict>
			<key>VAR_NAME</key>
			<string>$minuend</string>
			<key>MIN</key>
			<string>5</string>
			<key>MAX</key>
			<string>7</string>
		</dict>
		<dict>
			<key>VAR_NAME</key>
			<string>$subtrahend</string>
			<key>MIN</key>
			<string>3</string>
			<key>MAX</key>
			<string>{$minuend}</string>
		</dict>
	</array>
</dict>
</plist>
