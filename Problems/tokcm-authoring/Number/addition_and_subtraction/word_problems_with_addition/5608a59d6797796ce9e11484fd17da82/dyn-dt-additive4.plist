<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>TOOL_KEY</key>
	<string>DistributionTool</string>
	<key>DSTRINGS</key>
	<dict>
		<key>$object</key>
		<dict>
			<key>MODE</key>
			<string>RANDOM</string>
			<key>DATA</key>
			<array>
				<string>squid</string>
				<string>fish</string>
				<string>oysters</string>
				<string>shells</string>
			</array>
		</dict>
	</dict>
	<key>PROBLEM_DESCRIPTION</key>
	<string>Tom has {$augend} [[$object]], and Gareth has 10 [[$object]]. If Gareth gives Tom {$addend} [[$object]], how many [[$object]] does Tom have now?</string>
	<key>EVAL_MODE</key>
	<integer>1</integer>
	<key>DVARS</key>
	<array>
		<dict>
			<key>VAR_NAME</key>
			<string>$augend</string>
			<key>MIN</key>
			<string>7</string>
			<key>MAX</key>
			<string>15</string>
		</dict>
		<dict>
			<key>VAR_NAME</key>
			<string>$addend</string>
			<key>MIN</key>
			<string>2</string>
			<key>MAX</key>
			<string>5</string>
		</dict>
	</array>
	<key>INIT_OBJECTS</key>
	<array>
		<dict>
			<key>QUANTITY</key>
			<string>{$augend}</string>
			<key>POS_X</key>
			<integer>80</integer>
			<key>POS_Y</key>
			<integer>400</integer>
		</dict>
		<dict>
			<key>QUANTITY</key>
			<integer>10</integer>
			<key>POS_X</key>
			<integer>80</integer>
			<key>POS_Y</key>
			<integer>200</integer>
		</dict>
	</array>
	<key>SOLUTION</key>
	<array>
		<string>{$augend+$addend}</string>
		<string>{10-$addend}</string>
	</array>
</dict>
</plist>
