<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>MASTERY</key>
	<true/>
	<key>PROBLEM_DESCRIPTION</key>
	<string>There are {$var1} cups of juice with {$var2} ice cubes in each cup. Express the total number of ice cubes in an equation.</string>
	<key>EVAL_MODE</key>
	<integer>0</integer>
	<key>HOST_BACKGROUND_IMAGE</key>
	<string>/images/bg/bg-ipad-addition_2_7_9.png</string>
	<key>META_QUESTION</key>
	<dict>
		<key>META_QUESTION_COMPLETE_TEXT</key>
		<string>well done!!</string>
		<key>META_QUESTION_EVAL_MODE</key>
		<integer>0</integer>
		<key>META_QUESTION_ANSWER_MODE</key>
		<integer>0</integer>
		<key>META_QUESTION_ANSWERS</key>
		<array>
			<dict>
				<key>META_ANSWER_TEXT</key>
				<string>{$var1}-{$var2}</string>
				<key>META_ANSWER_VALUE</key>
				<false/>
			</dict>
			<dict>
				<key>META_ANSWER_TEXT</key>
				<string>{$var1}{$var2}</string>
				<key>META_ANSWER_VALUE</key>
				<false/>
			</dict>
			<dict>
				<key>META_ANSWER_TEXT</key>
				<string>{$var1}+{$var2}</string>
				<key>META_ANSWER_VALUE</key>
				<false/>
			</dict>
			<dict>
				<key>META_ANSWER_TEXT</key>
				<string>{$var1}x{$var2}</string>
				<key>META_ANSWER_VALUE</key>
				<true/>
			</dict>
		</array>
		<key>META_QUESTION_TITLE</key>
		<string>There are {$var1} cups of juice with {$var2} ice cubes in each cup. Express the total number of ice cubes in an equation.</string>
		<key>META_QUESTION_INCOMPLETE_TEXT</key>
		<string>Try again</string>
	</dict>
	<key>DVARS</key>
	<dict>
		<key>$var1</key>
		<dict>
			<key>MAX</key>
			<string>12</string>
			<key>MIN</key>
			<string>7</string>
		</dict>
		<key>$var2</key>
		<dict>
			<key>MAX</key>
			<string>7</string>
			<key>MIN</key>
			<string>2</string>
		</dict>
	</dict>
	<key>DSTRINGS</key>
	<dict>
		<key>@juice</key>
		<dict>
			<key>MODE</key>
			<string>RANDOM</string>
			<key>DATA</key>
			<array>
				<string>coke</string>
				<string>orange juice</string>
				<string>apple juice</string>
				<string>water</string>
				<string>mango juice</string>
			</array>
		</dict>
	</dict>
</dict>
</plist>
