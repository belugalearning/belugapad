<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>TOOL_KEY</key>
	<string>ExprBuilder</string>
	<key>EVAL_TYPE</key>
	<string>EXPRESSION_EQUALITIES</string>
	<key>EVAL_MODE</key>
	<integer>1</integer>
	<key>EXPR_STAGES</key>
	<array>
		<string>&lt;b:t&gt;Complete the equations.&lt;/b:t&gt;</string>
		<string>&lt;b:on number=&quot;{$z}&quot;/&gt;&lt;b:oo value=&quot;/&quot;/&gt;&lt;b:on number=&quot;{$y}&quot;/&gt;&lt;b:oo value=&quot;=&quot;/&gt;&lt;b:on number=&quot;{$x}&quot;/&gt;</string>
		<string>&lt;b:on number=&quot;{$z*0.1}&quot;/&gt;&lt;b:oo value=&quot;/&quot;/&gt;&lt;b:on number=&quot;{$y}&quot;/&gt;&lt;b:oo value=&quot;=&quot;/&gt;&lt;b:on usePicker=&quot;yes&quot;/&gt;</string>
		<string>&lt;b:on number=&quot;{$z*0.01}&quot;/&gt;&lt;b:oo value=&quot;/&quot;/&gt;&lt;b:on number=&quot;{$y}&quot;/&gt;&lt;b:oo value=&quot;=&quot;/&gt;&lt;b:on usePicker=&quot;yes&quot;/&gt;</string>
		<string>&lt;b:on number=&quot;{$z*0.001}&quot;/&gt;&lt;b:oo value=&quot;/&quot;/&gt;&lt;b:on number=&quot;{$y}&quot;/&gt;&lt;b:oo value=&quot;=&quot;/&gt;&lt;b:on usePicker=&quot;yes&quot;/&gt;</string>
	</array>
	<key>DVARS</key>
	<array>
		<dict>
			<key>VAR_NAME</key>
			<string>$x</string>
			<key>MIN</key>
			<string>6</string>
			<key>MAX</key>
			<string>12</string>
		</dict>
		<dict>
			<key>VAR_NAME</key>
			<string>$y</string>
			<key>MIN</key>
			<string>3</string>
			<key>MAX</key>
			<string>7</string>
		</dict>
		<dict>
			<key>VAR_NAME</key>
			<string>$z</string>
			<key>VALUE</key>
			<string>{$x*$y}</string>
		</dict>
	</array>
	<key>REPEAT_AS_IS_MIN</key>
	<string>2</string>
</dict>
</plist>
