<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>PROBLEM_DESCRIPTION</key>
	<string>Make a decimal equivalent to {$x}%</string>
	<key>TOOL_KEY</key>
	<string>PlaceValue</string>
	<key>COL_BASE_VALUE</key>
	<integer>10</integer>
	<key>FIRST_COL_VALUE</key>
	<integer>1</integer>
	<key>NUMBER_COLS</key>
	<integer>3</integer>
	<key>DEFAULT_COL</key>
	<integer>2</integer>
	<key>ROPES_PER_COL</key>
	<integer>2</integer>
	<key>ROWS_PER_COL</key>
	<integer>5</integer>
	<key>ALLOW_CONDENSING</key>
	<true/>
	<key>ALLOW_MULCHING</key>
	<false/>
	<key>ALLOW_PANNING</key>
	<false/>
	<key>SHOW_CAGE</key>
	<true/>
	<key>X_OFFSET</key>
	<real>0.3</real>
	<key>COLUMN_SPACING</key>
	<real>0.3</real>
	<key>SOLUTION</key>
	<dict>
		<key>SOLUTION_TYPE</key>
		<string>TOTAL_COUNT</string>
		<key>SOLUTION_VALUE</key>
		<string>{&amp;y}</string>
		<key>SOLUTION_DISPLAY_TEXT2</key>
		<string>success!</string>
	</dict>
	<key>REJECT_MODE</key>
	<integer>1</integer>
	<key>EVAL_MODE</key>
	<integer>1</integer>
	<key>DEFAULT_SCALE</key>
	<integer>1</integer>
	<key>COLUMN_SPRITES</key>
	<dict>
		<key>1</key>
		<string>/images/placevalue/obj-placevalue-unit1.png</string>
		<key>0.1</key>
		<string>/images/placevalue/obj-placevalue-unit1tenth.png</string>
		<key>0.01</key>
		<string>/images/placevalue/obj-placevalue-unit1hundredth.png</string>
	</dict>
	<key>DVARS</key>
	<array>
		<dict>
			<key>VAR_NAME</key>
			<string>$x</string>
			<key>MIN</key>
			<string>1</string>
			<key>MAX</key>
			<string>99</string>
		</dict>
		<dict>
			<key>VAR_NAME</key>
			<string>&amp;y</string>
			<key>VALUE</key>
			<string>{$x*0.01}</string>
		</dict>
	</array>
	<key>REPEAT_AS_IS_MIN</key>
	<string>3</string>
</dict>
</plist>
