<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>PROBLEM_DESCRIPTION</key>
	<string>show that {$first_value}+{$second_value} = {$target}</string>
	<key>INTERNAL_DESCRIPTION</key>
	<string>show that x+y=x+y</string>
	<key>TOOL_KEY</key>
	<string>NumberBonds</string>
	<key>INIT_OBJECTS</key>
	<array>
		<dict>
			<key>PUT_IN_ROW</key>
			<integer>0</integer>
			<key>LENGTH</key>
			<string>{$target}</string>
		</dict>
	</array>
	<key>INIT_BARS</key>
	<array>
		<dict>
			<key>LOCKED</key>
			<true/>
			<key>LENGTH</key>
			<string>{$target}</string>
		</dict>
		<dict>
			<key>LOCKED</key>
			<false/>
			<key>LENGTH</key>
			<string>{$target}</string>
		</dict>
	</array>
	<key>INIT_CAGES</key>
	<array>
		<dict>
			<key>LENGTH</key>
			<string>{$first_value}</string>
			<key>LABEL</key>
			<string>{$first_value}</string>
			<key>QUANTITY</key>
			<integer>1</integer>
		</dict>
		<dict>
			<key>LENGTH</key>
			<string>{$second_value}</string>
			<key>LABEL</key>
			<string>{$second_value}</string>
			<key>QUANTITY</key>
			<string>1</string>
		</dict>
	</array>
	<key>DVARS</key>
	<array>
		<dict>
			<key>VAR_NAME</key>
			<string>$first_value</string>
			<key>MIN</key>
			<string>1</string>
			<key>MAX</key>
			<string>5</string>
		</dict>
		<dict>
			<key>VAR_NAME</key>
			<string>$second_value</string>
			<key>MIN</key>
			<string>1</string>
			<key>MAX</key>
			<string>5</string>
		</dict>
		<dict>
			<key>VAR_NAME</key>
			<string>$target</string>
			<key>VALUE</key>
			<string>{$first_value+$second_value}</string>
		</dict>
	</array>
	<key>EVAL_MODE</key>
	<integer>1</integer>
	<key>REJECT_MODE</key>
	<integer>0</integer>
</dict>
</plist>
