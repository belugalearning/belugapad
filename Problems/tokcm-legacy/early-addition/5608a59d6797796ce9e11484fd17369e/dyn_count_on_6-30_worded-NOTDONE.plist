<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>PROBLEM_DESCRIPTION</key>
	<string>[[$name]] has {$start_value} [[$object]] and gets {$interval} more. How many does [[$name]] have?</string>
	<key>INTERNAL_DESCRIPTION</key>
	<string>Tom has n objects and gets some more. How many do they have?</string>
	<key>TOOL_KEY</key>
	<string>NLine</string>
	<key>START_VALUE</key>
	<string>{$start_value}</string>
	<key>MIN_VALUE</key>
	<string>0</string>
	<key>MAX_VALUE</key>
	<string>50</string>
	<key>EVAL_MODE</key>
	<integer>1</integer>
	<key>REJECT_MODE</key>
	<integer>0</integer>
	<key>EVAL_TARGET</key>
	<string>{$start_value+$interval}</string>
	<key>DVARS</key>
	<dict>
		<key>$start_value</key>
		<dict>
			<key>MIN</key>
			<string>6</string>
			<key>MAX</key>
			<string>30</string>
		</dict>
		<key>$interval</key>
		<dict>
			<key>MIN</key>
			<string>1</string>
			<key>MAX</key>
			<string>19</string>
		</dict>
	</dict>
	<key>DSTRINGS</key>
	<dict>
		<key>$name</key>
		<dict>
			<key>MODE</key>
			<string>RANDOM</string>
			<key>DATA</key>
			<array>
				<string>Tom</string>
				<string>Kim</string>
			</array>
		</dict>
		<key>$object</key>
		<dict>
			<key>MODE</key>
			<string>RANDOM</string>
			<key>DATA</key>
			<array>
				<string>ropes</string>
				<string>fish</string>
				<string>ice cubes</string>
			</array>
		</dict>
	</dict>
	<key>HIDE_ALL_NUMBERS</key>
	<false/>
	<key>SHOW_NUMBERS_AT_INTERVALS</key>
	<array>
		<integer>10</integer>
	</array>
	<key>HIDE_ALL_NOTCHES</key>
	<false/>
	<key>SHOW_NOTCHES_AT_INTERVALS</key>
	<array/>
	<key>HIDE_START_NUMBER</key>
	<false/>
	<key>HIDE_END_NUMBER</key>
	<false/>
	<key>HIDE_START_NOTCH</key>
	<false/>
	<key>HIDE_END_NOTCH</key>
	<false/>
	<key>DISABLE_SNAPPING_TO_NOTCH</key>
	<false/>
	<key>EVAL_PLUS_MINUS_THRESHOLD</key>
	<integer>0</integer>
	<key>COUNT_OUT_LOUD_AT_INTERVALS</key>
	<array>
		<integer>1</integer>
	</array>
	<key>SHOW_COUNT_HISTORY_AT_INTERVALS</key>
	<array/>
	<key>USER_POPULATED_NUMBERS</key>
	<array>
		<string>2</string>
		<string>4</string>
		<string>6</string>
		<string>8</string>
	</array>
	<key>RENDER_STITCHES_AT_INTERVAL</key>
	<integer>0</integer>
</dict>
</plist>
