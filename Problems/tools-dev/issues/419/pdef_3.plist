<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>PROBLEM_DESCRIPTION</key>
	<string>Show how much {@larger} 8 is than 5 and 3.</string>
	<key>INTERNAL_DESCRIPTION</key>
	<string>Show that 8 is larger than 5 by 3.</string>
	<key>TOOL_KEY</key>
	<string>NumberBonds</string>
	<key>INIT_BARS</key>
	<array>
		<dict>
			<key>LOCKED</key>
			<true/>
			<key>LENGTH</key>
			<integer>8</integer>
		</dict>
		<dict>
			<key>LOCKED</key>
			<false/>
			<key>LENGTH</key>
			<integer>8</integer>
		</dict>
		<dict>
			<key>LOCKED</key>
			<false/>
			<key>LENGTH</key>
			<integer>8</integer>
		</dict>
	</array>
	<key>INIT_OBJECTS</key>
	<array>
		<dict>
			<key>LABEL</key>
			<string>8</string>
			<key>PUT_IN_ROW</key>
			<integer>0</integer>
			<key>LENGTH</key>
			<integer>8</integer>
		</dict>
		<dict>
			<key>LABEL</key>
			<string>5</string>
			<key>PUT_IN_ROW</key>
			<integer>1</integer>
			<key>LENGTH</key>
			<integer>5</integer>
		</dict>
		<dict>
			<key>LABEL</key>
			<string>3</string>
			<key>PUT_IN_ROW</key>
			<integer>2</integer>
			<key>LENGTH</key>
			<integer>3</integer>
		</dict>
	</array>
	<key>EVAL_MODE</key>
	<integer>1</integer>
	<key>INIT_CAGES</key>
	<array>
		<dict>
			<key>LENGTH</key>
			<integer>1</integer>
			<key>LABEL</key>
			<string>1</string>
			<key>QUANTITY</key>
			<integer>10</integer>
		</dict>
		<dict>
			<key>LENGTH</key>
			<integer>2</integer>
			<key>LABEL</key>
			<string>2</string>
			<key>QUANTITY</key>
			<integer>10</integer>
		</dict>
		<dict>
			<key>LENGTH</key>
			<integer>3</integer>
			<key>LABEL</key>
			<string>3</string>
			<key>QUANTITY</key>
			<integer>10</integer>
		</dict>
	</array>
	<key>SHOW_BADGES</key>
	<false/>
	<key>DSTRINGS</key>
	<dict>
		<key>@larger</key>
		<dict>
			<key>MODE</key>
			<string>RANDOM</string>
			<key>DATA</key>
			<array>
				<string>larger</string>
				<string>bigger</string>
				<string>greater</string>
			</array>
		</dict>
	</dict>
</dict>
</plist>
