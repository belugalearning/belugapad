<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>PROBLEM_DESCRIPTION</key>
	<string>Make {$answer}.</string>
	<key>TOOL_KEY</key>
	<string>PlaceValue</string>
	<key>NUMBER_COLS</key>
	<integer>1</integer>
	<key>FIRST_COL_VALUE</key>
	<integer>1</integer>
	<key>COL_BASE_VALUE</key>
	<string>10</string>
	<key>DEFAULT_COL</key>
	<integer>1</integer>
	<key>ROPES_PER_COL</key>
	<integer>5</integer>
	<key>ROWS_PER_COL</key>
	<integer>4</integer>
	<key>ALLOW_CONDENSING</key>
	<false/>
	<key>ALLOW_MULCHING</key>
	<false/>
	<key>EVAL_MODE</key>
	<integer>1</integer>
	<key>REJECT_MODE</key>
	<integer>0</integer>
	<key>SHOW_CAGE</key>
	<true/>
	<key>SHOW_COUNT</key>
	<false/>
	<key>SHOW_COUNT_BLOCK</key>
	<true/>
	<key>COLUMN_NEG_CAGES</key>
	<dict>
		<key>1</key>
		<true/>
	</dict>
	<key>INIT_OBJECTS</key>
	<array>
		<dict>
			<key>PUT_IN_COL</key>
			<integer>0</integer>
			<key>NUMBER_NEGATIVE</key>
			<string>{0-$x}</string>
		</dict>
	</array>
	<key>SOLUTION</key>
	<dict>
		<key>SOLUTION_TYPE</key>
		<string>TOTAL_COUNT</string>
		<key>SOLUTION_VALUE</key>
		<string>{$answer}</string>
		<key>INCOMPLETE_DISPLAY_TEXT</key>
		<string>solution incomplete, try again.</string>
	</dict>
	<key>DEFAULT_SCALE</key>
	<integer>1</integer>
	<key>IS_NEGATIVE_PROBLEM</key>
	<true/>
	<key>CAGE_DEFAULT_VALUE</key>
	<integer>-1</integer>
	<key>EXPLODE_MODE</key>
	<true/>
	<key>SHOW_BASE_SELECTION</key>
	<true/>
	<key>SHOW_MORE_LESS_ARROWS</key>
	<false/>
	<key>SHOW_COLUMN_USER_COUNT</key>
	<true/>
	<key>SHOW_VALUE</key>
	<false/>
	<key>SHOW_COLUMN_TOTAL_COUNT</key>
	<false/>
	<key>DVARS</key>
	<array>
		<dict>
			<key>VAR_NAME</key>
			<string>$x</string>
			<key>MIN</key>
			<string>-5</string>
			<key>MAX</key>
			<string>0</string>
		</dict>
		<dict>
			<key>VAR_NAME</key>
			<string>$a</string>
			<key>MIN</key>
			<string>2</string>
			<key>MAX</key>
			<string>7</string>
		</dict>
		<dict>
			<key>VAR_NAME</key>
			<string>$answer</string>
			<key>VALUE</key>
			<string>{$x-$a}</string>
		</dict>
	</array>
</dict>
</plist>
