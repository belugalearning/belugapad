<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>PROBLEM_DESCRIPTION</key>
	<string>What is {$x} x {$y*10} ?</string>
	<key>SHOW_NUMBERWHEEL_FOR_SHAPES</key>
	<true/>
	<key>DVARS</key>
	<array>
		<dict>
			<key>MAX</key>
			<string>50</string>
			<key>RECALL</key>
			<string>{$x}</string>
			<key>VAR_NAME</key>
			<string>$x</string>
			<key>MIN</key>
			<string>30</string>
		</dict>
		<dict>
			<key>MAX</key>
			<string>8</string>
			<key>RECALL</key>
			<string>{$y}</string>
			<key>VAR_NAME</key>
			<string>$y</string>
			<key>MIN</key>
			<string>3</string>
		</dict>
	</array>
	<key>EVAL_MODE</key>
	<string>1</string>
	<key>ANCHOR_SPACE</key>
	<integer>85</integer>
	<key>AUTO_UPDATE_WHEEL</key>
	<true/>
	<key>DOTGRID_EVAL_NONPROP_X</key>
	<string>{$x}</string>
	<key>DOTGRID_EVAL_NONPROP_Y</key>
	<string>{$y*10}</string>
	<key>DO_NOT_SIMPLIFY_FRACTIONS</key>
	<false/>
	<key>DOTGRID_EVAL_TYPE</key>
	<integer>5</integer>
	<key>MASTERY</key>
	<true/>
	<key>TOOL_KEY</key>
	<string>DotGrid</string>
	<key>DRAW_MODE</key>
	<integer>0</integer>
</dict>
</plist>
