			</dict>
			<dict>
				<key>META_ANSWER_TEXT</key>
				<string>{$ans+1}</string>
				<key>META_ANSWER_VALUE</key>
				<false/>
			</dict>
			<dict>
				<key>META_ANSWER_TEXT</key>
				<string>{$ans-1}</string>
				<key>META_ANSWER_VALUE</key>
				<false/>
