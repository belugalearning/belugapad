<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>MASTERY</key>
	<true/>
	<key>TOOL_KEY</key>
	<string>ExprBuilder</string>
	<key>INTERNAL_DESCRIPTION</key>
	<string>{$x}+{$y}</string>
	<key>EVAL_MODE</key>
	<string>1</string>
	<key>EVAL_TYPE</key>
	<string>EXPRESSION_EQUALITIES</string>
	<key>EXPR_STAGES</key>
	<array>
		<string>&lt;b:oi tag=&quot;{@thing_1}&quot;/&gt;&lt;b:t&gt;has&lt;/b:t&gt;&lt;b:on number=&quot;{$x}&quot;/&gt;&lt;b:t&gt;objects and is given&lt;/b:t&gt;&lt;b:on number=&quot;{$y}&quot;/&gt;&lt;b:t&gt;more. What number is missing in the equation?&lt;/b:t&gt; </string>
		<string>&lt;b:on number=&quot;{$x}&quot; enabled=&quot;yes&quot;/&gt;
&lt;b:oo value=&quot;+&quot; /&gt;
&lt;b:on number=&quot;{$y}&quot; enabled=&quot;yes&quot;/&gt;
&lt;b:oo value=&quot;=&quot; /&gt;
&lt;b:on usePicker=&quot;yes&quot; /&gt;</string>
	</array>
	<key>DVARS</key>
	<array>
		<dict>
			<key>RECALL</key>
			<string>{$x}</string>
			<key>VAR_NAME</key>
			<string>$x</string>
			<key>MIN</key>
			<string>4</string>
			<key>MAX</key>
			<string>8</string>
		</dict>
		<dict>
			<key>VAR_NAME</key>
			<string>$y</string>
			<key>RECALL</key>
			<string>{$y}</string>
			<key>MIN</key>
			<string>6</string>
			<key>MAX</key>
			<string>12</string>
		</dict>
		<dict>
			<key>VAR_NAME</key>
			<string>$z</string>
			<key>VALUE</key>
			<string>{$x+$y}</string>
		</dict>
	</array>
	<key>DSTRINGS</key>
	<dict>
		<key>@first</key>
		<dict>
			<key>MODE</key>
			<string>RANDOM</string>
			<key>DATA</key>
			<array>
				<string>at first</string>
				<string>to start with</string>
			</array>
		</dict>
		<key>@thing_1</key>
		<dict>
			<key>MODE</key>
			<string>RANDOM</string>
			<key>DATA</key>
			<array>
				<string>thing_1</string>
				<string>thing_2</string>
				<string>thing_3</string>
			</array>
		</dict>
		<key>@thing_2</key>
		<dict>
			<key>MODE</key>
			<string>RANDOM</string>
			<key>DATA</key>
			<array>
				<string>thing_4</string>
				<string>thing_5</string>
			</array>
		</dict>
	</dict>
	<key>REPEAT_SET</key>
	<string>A</string>
	<key>REPEAT_MY_SET_MIN</key>
	<integer>2</integer>
</dict>
</plist>
