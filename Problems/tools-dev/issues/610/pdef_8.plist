<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>MASTERY</key>
	<true/>
	<key>TOOL_KEY</key>
	<string>ExprBuilder</string>
	<key>INTERNAL_DESCRIPTION</key>
	<string>1</string>
	<key>EVAL_MODE</key>
	<string>1</string>
	<key>EVAL_TYPE</key>
	<string>ALL_ENABLED</string>
	<key>EXPR_STAGES</key>
	<array>
		<string>&lt;b:t&gt;Fill in the gaps.&lt;/b:t&gt;</string>
		<string>&lt;b:ot tag=&quot;a&quot;&gt;{$x}&lt;/b:ot&gt;&lt;b:t&gt;x&lt;/b:t&gt;&lt;b:ot tag=&quot;b&quot;&gt;{$y}&lt;/b:ot&gt;&lt;b:t&gt;=&lt;/b:t&gt;&lt;b:ot tag=&quot;c&quot;&gt;{$z}&lt;/b:ot&gt;</string>
		<string>&lt;b:oi tag=&quot;{@thing_1}&quot;/&gt;&lt;b:t&gt;earns&lt;/b:t&gt;&lt;b:ph targetTag=&quot;a&quot;/&gt;&lt;b:obj count=&quot;2&quot;/&gt;&lt;b:t&gt; per day.&lt;/b:t&gt;
&lt;b:oi tag=&quot;{@thing_2}&quot;/&gt;&lt;b:t&gt;earns&lt;/b:t&gt;&lt;b:ph targetTag=&quot;b&quot;/&gt;&lt;b:t&gt;times as much.&lt;/b:t&gt;
&lt;b:t&gt; So&lt;/b:t&gt;&lt;b:oi tag=&quot;{@thing_2}&quot;/&gt;&lt;b:t&gt;earns&lt;/b:t&gt;&lt;b:ph targetTag=&quot;c&quot;/&gt;&lt;b:t&gt;goobles per day.&lt;/b:t&gt;</string>
	</array>
	<key>DVARS</key>
	<array>
		<dict>
			<key>VAR_NAME</key>
			<string>$x</string>
			<key>MIN</key>
			<string>3</string>
			<key>MAX</key>
			<string>7</string>
		</dict>
		<dict>
			<key>VAR_NAME</key>
			<string>$y</string>
			<key>MIN</key>
			<string>2</string>
			<key>MAX</key>
			<string>5</string>
		</dict>
		<dict>
			<key>VAR_NAME</key>
			<string>$z</string>
			<key>VALUE</key>
			<string>{$x*$y}</string>
		</dict>
	</array>
	<key>DSTRINGS</key>
	<dict>
		<key>@thing_1</key>
		<dict>
			<key>MODE</key>
			<string>RANDOM</string>
			<key>DATA</key>
			<array>
				<string>thing_1</string>
				<string>thing_2</string>
				<string>thing_3</string>
			</array>
		</dict>
		<key>@thing_2</key>
		<dict>
			<key>MODE</key>
			<string>RANDOM</string>
			<key>DATA</key>
			<array>
				<string>thing_4</string>
				<string>thing_5</string>
			</array>
		</dict>
	</dict>
	<key>REPEAT_SET</key>
	<string>a</string>
</dict>
</plist>
