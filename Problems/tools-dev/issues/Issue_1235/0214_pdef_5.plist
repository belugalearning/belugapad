<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>EVAL_TYPE</key>
    <string>SEQUENCE_ASC</string>
    <key>INTERNAL_DESCRIPTION</key>
    <string>1 to 10</string>
    <key>DVARS</key>
    <dict>
      <key>$x</key>
      <dict>
        <key>MIN</key>
        <string>11</string>
        <key>MAX</key>
        <string>15</string>
      </dict>
    </dict>
    <key>EVAL_MODE</key>
    <string>1</string>
    <key>NUMBER_CARD_ROW_MAX</key>
    <string>{$x+5}</string>
    <key>NUMBER_CARD_INTERVAL</key>
    <integer>1</integer>
    <key>NUMBER_CARD_RANDOMISE</key>
    <true/>
    <key>NUMBER_CARD_ROW_MIN</key>
    <string>{$x+1}</string>
    <key>EXPR_STAGES</key>
    <array>
      <string>&lt;b:t&gt;Sort these numbers in the correct order.&lt;/b:t&gt;</string>
      <string>&lt;b:ph/&gt;&lt;b:ph/&gt;&lt;b:ph/&gt;&lt;b:on number=&quot;{$x+4}&quot;/&gt;&lt;b:ph/&gt;</string>
    </array>
    <key>REPEAT_AS_IS_MIN</key>
    <integer>2</integer>
    <key>TOOL_KEY</key>
    <string>ExprBuilder</string>
  </dict>
</plist>
