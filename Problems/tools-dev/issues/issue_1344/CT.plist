<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>PROBLEM_DESCRIPTION</key>
    <string>&lt;b:t&gt;Press the&lt;/b:t&gt;&lt;b:oi tag=&quot;30_Counter_Stop&quot;/&gt;&lt;b:t&gt;at {$x}.&lt;/b:t&gt;</string>
    <key>TOOL_KEY</key>
    <string>CountingTimer</string>
    <key>SOLUTION</key>
    <string>{$x}</string>
    <key>SHOW_COUNT</key>
    <true/>
    <key>COUNT_TYPE</key>
    <integer>2</integer>
    <key>INCREMENT</key>
    <integer>1</integer>
    <key>COUNT_MIN</key>
    <string>0</string>
    <key>COUNT_MAX</key>
    <string>{$x+3}</string>
    <key>USE_NUMICON_NUMBER</key>
    <true/>
    <key>DVARS</key>
    <array>
      <dict>
        <key>VAR_NAME</key>
        <string>$x</string>
        <key>MIN</key>
        <string>7</string>
        <key>MAX</key>
        <string>10</string>
      </dict>
      <dict>
        <key>VAR_NAME</key>
        <string>$y</string>
        <key>MIN</key>
        <string>11</string>
        <key>MAX</key>
        <string>20</string>
      </dict>
      <dict>
        <key>VAR_NAME</key>
        <string>$z</string>
        <key>VALUE</key>
        <string>{$x+$y}</string>
      </dict>
    </array>
  </dict>
</plist>
