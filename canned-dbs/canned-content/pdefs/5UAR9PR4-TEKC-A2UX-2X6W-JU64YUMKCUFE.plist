<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>FLASHING_BUTTON</key>
	<true/>
	<key>MASTERY</key>
	<true/>
	<key>PROBLEM_DESCRIPTION</key>
	<string>Each click represents 100. Starting at {$beginning} stop the counter when you have counted on {$n} hundreds.</string>
	<key>TOOL_KEY</key>
	<string>CountingTimer</string>
	<key>SHOW_COUNT</key>
	<true/>
	<key>COUNT_TYPE</key>
	<integer>1</integer>
	<key>INCREMENT</key>
	<integer>100</integer>
	<key>COUNT_MIN</key>
	<string>{$beginning}</string>
	<key>COUNT_MAX</key>
	<string>{$total}</string>
	<key>DVARS</key>
	<array>
		<dict>
			<key>VAR_NAME</key>
			<string>$var1</string>
			<key>RETAIN</key>
			<true/>
			<key>MIN</key>
			<string>0</string>
			<key>MAX</key>
			<string>5</string>
		</dict>
		<dict>
			<key>VAR_NAME</key>
			<string>$n</string>
			<key>RETAIN</key>
			<true/>
			<key>MIN</key>
			<string>1</string>
			<key>MAX</key>
			<string>4</string>
		</dict>
		<dict>
			<key>VAR_NAME</key>
			<string>$var2</string>
			<key>RETAIN</key>
			<true/>
			<key>MIN</key>
			<string>0</string>
			<key>MAX</key>
			<string>9</string>
		</dict>
		<dict>
			<key>VAR_NAME</key>
			<string>$var3</string>
			<key>RETAIN</key>
			<true/>
			<key>MIN</key>
			<string>0</string>
			<key>MAX</key>
			<string>9</string>
		</dict>
		<dict>
			<key>VAR_NAME</key>
			<string>$ten</string>
			<key>VALUE</key>
			<string>{$var2*10}</string>
		</dict>
		<dict>
			<key>VAR_NAME</key>
			<string>$nhundred</string>
			<key>VALUE</key>
			<string>{$n*100}</string>
		</dict>
		<dict>
			<key>VAR_NAME</key>
			<string>$hundred</string>
			<key>VALUE</key>
			<string>{$var1*100}</string>
		</dict>
		<dict>
			<key>VAR_NAME</key>
			<string>$hundredsandtens</string>
			<key>VALUE</key>
			<string>{$hundred+$ten}</string>
		</dict>
		<dict>
			<key>VAR_NAME</key>
			<string>$hundredandtenandn</string>
			<key>VALUE</key>
			<string>{$hundredsandtens+$nhundred}</string>
		</dict>
		<dict>
			<key>VAR_NAME</key>
			<string>$total</string>
			<key>VALUE</key>
			<string>{$hundredandtenandn+$var3}</string>
		</dict>
		<dict>
			<key>VAR_NAME</key>
			<string>$beginning</string>
			<key>VALUE</key>
			<string>{$hundredsandtens+$var3}</string>
		</dict>
	</array>
	<key>NUMBER_PICKER</key>
	<dict>
		<key>NUMBER_PICKER_DESCRIPTION</key>
		<string>If you starting at {$beginning} and each click represents 100, what number are you on after {$n} clicks?</string>
		<key>PICKER_LAYOUT</key>
		<integer>1</integer>
		<key>ANIMATE_FROM_PICKER</key>
		<true/>
		<key>SHOW_DROPBOX</key>
		<true/>
		<key>PICKER_EVAL_MODE</key>
		<integer>1</integer>
		<key>EVAL_VALUE</key>
		<string>{$total}</string>
	</dict>
</dict>
</plist>
