		</dict>
	</array>
	<key>DSTRINGS</key>
	<dict>
		<key>@subtract</key>
		<dict>
			<key>MODE</key>
			<string>RANDOM</string>
			<key>DATA</key>
			<array>
				<string>Take away</string>
				<string>Subtract</string>
			</array>
		</dict>
