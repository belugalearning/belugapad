<dict>
	<key>TOOL_KEY</key>
	<string>ExprBuilder</string>
	<key>PROBLEM_DESCRIPTION</key>
	<string>Write an equation to express this.</string>
	<key>INTERNAL_DESCRIPTION</key>
	<string>1</string>
	<key>EXPR_STAGES</key>
	<array>
		<string>&lt;b:ot tag=&quot;a&quot;&gt;21&lt;/b:ot&gt;
&lt;b:t&gt;is&lt;/b:t&gt;
&lt;b:ot tag=&quot;b&quot;&gt;3&lt;/b:ot&gt;
&lt;b:t&gt;times larger than&lt;/b:t&gt;
&lt;b:ot tag=&quot;c&quot;&gt;7.&lt;/b:ot&gt;</string>
		<string>&lt;b:commlkp lkpid=&quot;cl1&quot;&gt;
 &lt;b:lkpitem tag=&quot;a&quot;&gt;21&lt;/b:lkpitem&gt;
 &lt;b:lkpitem tag=&quot;b&quot;&gt;3&lt;/b:lkpitem&gt;
 &lt;b:lkpitem tag=&quot;c&quot;&gt;7&lt;/b:lkpitem&gt;
&lt;/b:commlkp&gt;

&lt;b:commot lkpid=&quot;cl1&quot; sample=&quot;3&quot; preftag=&quot;b&quot; /&gt;
&lt;b:op op=&quot;x&quot; picker=&quot;yes&quot; /&gt;
&lt;b:commot lkpid=&quot;cl1&quot; sample=&quot;7&quot; preftag=&quot;c&quot; /&gt;
&lt;b:op op=&quot;=&quot; picker=&quot;yes&quot; /&gt;
&lt;b:commot lkpid=&quot;cl1&quot; sample=&quot;21&quot; preftag=&quot;a&quot; /&gt;</string>
	</array>
	<key>DVARS</key>
