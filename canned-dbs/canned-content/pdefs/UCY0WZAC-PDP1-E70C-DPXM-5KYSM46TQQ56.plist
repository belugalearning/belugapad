<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>REPEAT_SET</key>
	<string>COUNTING BACK 10 OR 100</string>
	<key>CORE</key>
	<true/>
	<key>PROBLEM_DESCRIPTION</key>
	<string>This is {$aactual}. {@aname} 100.</string>
	<key>INTERNAL_DESCRIPTION</key>
	<string>subbing 100 from xxx</string>
	<key>TOOL_KEY</key>
	<string>PlaceValue</string>
	<key>COL_BASE_VALUE</key>
	<integer>10</integer>
	<key>FIRST_COL_VALUE</key>
	<integer>100</integer>
	<key>NUMBER_COLS</key>
	<integer>3</integer>
	<key>DEFAULT_COL</key>
	<integer>2</integer>
	<key>ROPES_PER_COL</key>
	<integer>2</integer>
	<key>ROWS_PER_COL</key>
	<integer>5</integer>
	<key>ALLOW_CONDENSING</key>
	<true/>
	<key>ALLOW_MULCHING</key>
	<true/>
	<key>ALLOW_PANNING</key>
	<false/>
	<key>SHOW_CAGE</key>
	<true/>
	<key>X_OFFSET</key>
	<real>0.3</real>
	<key>DEFAULT_SCALE</key>
	<real>0.9</real>
	<key>COLUMN_SPACING</key>
	<real>0.3</real>
	<key>SHOW_NEG_CAGE</key>
	<true/>
	<key>SHOW_COL_HEADER</key>
	<false/>
	<key>SHOW_VALUE</key>
	<true/>
	<key>SHOW_BASE_SELECTION</key>
	<true/>
	<key>PICKUP_SPRITE_FILENAME</key>
	<string>/images/placevalue/cage-neg.png</string>
	<key>PROXIMITY_SPRITE_FILENAME</key>
	<string>/images/placevalue/cage-pos.png</string>
	<key>CUSTOM_COLUMN_HEADERS</key>
	<dict>
		<key>100</key>
		<string>hundreds</string>
		<key>10</key>
		<string>tens</string>
		<key>1</key>
		<string>ones</string>
	</dict>
	<key>INIT_OBJECTS</key>
	<array>
		<dict>
			<key>PUT_IN_COL</key>
			<integer>0</integer>
			<key>NUMBER</key>
			<string>{$avar1}</string>
		</dict>
		<dict>
			<key>PUT_IN_COL</key>
			<integer>1</integer>
			<key>NUMBER</key>
			<string>{$avar2}</string>
		</dict>
		<dict>
			<key>PUT_IN_COL</key>
			<integer>2</integer>
			<key>NUMBER</key>
			<string>{$avar3}</string>
		</dict>
	</array>
	<key>REJECT_MODE</key>
	<integer>1</integer>
	<key>EVAL_MODE</key>
	<integer>1</integer>
	<key>COLUMN_SPRITES</key>
	<dict>
		<key>100</key>
		<string>/images/placevalue/obj-placevalue-unit100.png</string>
		<key>10</key>
		<string>/images/placevalue/obj-placevalue-unit10.png</string>
		<key>1</key>
		<string>/images/placevalue/obj-placevalue-unit1.png</string>
	</dict>
	<key>COLUMN_CAGES</key>
	<dict>
		<key>100</key>
		<true/>
		<key>10</key>
		<true/>
		<key>1</key>
		<true/>
	</dict>
	<key>COLUMN_NEG_CAGES</key>
	<dict>
		<key>10</key>
		<false/>
		<key>1</key>
		<false/>
	</dict>
	<key>DVARS</key>
	<array>
		<dict>
			<key>RECALL</key>
			<string>{$avar1}</string>
			<key>VAR_NAME</key>
			<string>$avar1</string>
			<key>MIN</key>
			<string>1</string>
			<key>MAX</key>
			<string>9</string>
		</dict>
		<dict>
			<key>RECALL</key>
			<string>{$avar2}</string>
			<key>VAR_NAME</key>
			<string>$avar2</string>
			<key>MIN</key>
			<string>1</string>
			<key>MAX</key>
			<string>8</string>
		</dict>
		<dict>
			<key>RECALL</key>
			<string>{$avar3}</string>
			<key>VAR_NAME</key>
			<string>$avar3</string>
			<key>MIN</key>
			<string>1</string>
			<key>MAX</key>
			<string>9</string>
		</dict>
		<dict>
			<key>VAR_NAME</key>
			<string>$ahundred</string>
			<key>VALUE</key>
			<string>{$avar1*100}</string>
		</dict>
		<dict>
			<key>VAR_NAME</key>
			<string>$aten</string>
			<key>VALUE</key>
			<string>{$avar2*10}</string>
		</dict>
		<dict>
			<key>VAR_NAME</key>
			<string>$ahundredsandtens</string>
			<key>VALUE</key>
			<string>{$ahundred+$aten}</string>
		</dict>
		<dict>
			<key>VAR_NAME</key>
			<string>$aactual</string>
			<key>VALUE</key>
			<string>{$ahundredsandtens+$avar3}</string>
		</dict>
		<dict>
			<key>VAR_NAME</key>
			<string>$total</string>
			<key>VALUE</key>
			<string>{$aactual-100}</string>
		</dict>
	</array>
	<key>DSTRINGS</key>
	<dict>
		<key>@aname</key>
		<dict>
			<key>MODE</key>
			<string>RANDOM</string>
			<key>DATA</key>
			<array>
				<string>Take away</string>
				<string>Subtract</string>
				<string>Decrease this by</string>
			</array>
		</dict>
	</dict>
	<key>NUMBER_PICKER</key>
	<dict>
		<key>NUMBER_PICKER_DESCRIPTION</key>
		<string>This is {$aactual} , {@aname} 100.</string>
		<key>PICKER_LAYOUT</key>
		<integer>1</integer>
		<key>ANIMATE_FROM_PICKER</key>
		<true/>
		<key>SHOW_DROPBOX</key>
		<true/>
		<key>PICKER_EVAL_MODE</key>
		<integer>1</integer>
		<key>EVAL_VALUE</key>
		<string>{$total}</string>
	</dict>
</dict>
</plist>
